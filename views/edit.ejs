<% include ./partials/header %>


    <br><br><br>







    <br><br>
<br><br>

    <div class="container-fluid" style="padding: 0px 80px;font-size: 18px !important;">  
            <form action="/users/event/edit/<%=notf._id%>?_method=PUT" method="POST">
                <div class="container-fluid" style="margin-left: -15px;">
                 <div class="row">
                  <div class="col-md-6">
                <div class="form-group">
                  <label>Event Title:</label>
                  <input type="text" name="title" class="form-control" value="<%=notf.title%>" required="">
                </div>
                  </div>
                  <div class="col-md-5">
                <div class="form-group">
                  <label>Date:</label>
                  <input type="text" name="date" id = "#datepicker" class="form-control" value="<%=notf.date%>" required="">
                </div>
                </div>
              </div>
              <div class="row">
                  
                  <div class="col-md-6">
                          <div class="form-group">
                                  <label>Description:</label>
                                  <input type="text" name="description" class="form-control" value="<%=notf.description%>" required="">
                                </div>
                  </div>
                  <div class="col-md-5">
                          <div class="form-group">
                                  <label>Time:</label>
                                  <input type="text" name="time" id = "#time" class="form-control" value="<%=notf.time%>" required="">
                                </div>
                  </div>
      
              </div>
      
              <div class="row">
                      <div class="col-md-6">
                              <div class="form-group">
                                      <label>Venue:</label>
                                      <input type="text" name="venue" class="form-control" value="<%=notf.venue%>" required="">
                                    </div>
                      </div>
                      <div class="col-md-1" style="padding-left:0px;">
                            <label style="color: white;">.</label><br>
                            <button type="submit" class="btn dropbtnLoginc save-btn">Update</button>
                      </div>
              </div>
                    
               
            
               
              </div>    
              </form>  
        
        <br/>
        <table  id="customers2"class="table data-table">
                <thead>
                  <th width="20%">Event Title</th>
                  <th width="10%">Date</th>
                  <th width="10%">Time</th>
                  <th width="30%">Description</th>
                  <th width="20%">Venue</th>
                </thead>
                <tbody>
                    <% if(event.length!=0){
                        event.forEach((fnot)=>{%>
                          <tr>
                                  <td><%=fnot.title%></td>
                                  <td><%=fnot.date%> </td>
                                  <td><%=fnot.time%> </td>
                                  <td><%=fnot.description%> </td>
                                  <td><%=fnot.venue%> </td>
                                    
                                  </tr>
                       <% })
                    }%>
                 
                  </tbody>
              </table>
    </div>
